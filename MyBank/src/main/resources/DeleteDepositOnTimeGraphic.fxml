   <children>
      <Label layoutX="50.0" layoutY="23.0" text="Choose id of deposit you want to delete">
         <font>
            <Font size="18.0" />
         </font></Label>
      <TextField fx:id="txtIdToDelete" layoutX="88.0" layoutY="57.0" prefHeight="39.0" prefWidth="97.0" promptText=" id here">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="btnDeleteDeposit" layoutX="216.0" layoutY="57.0" mnemonicParsing="false" onAction="#handleDeleteDeposit" text="Delete">
         <font>
            <Font size="18.0" />
         </font></Button>
   </children>
    
</AnchorPane>
