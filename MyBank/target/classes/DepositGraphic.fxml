<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="221.0" prefWidth="444.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraphicInterface.DepositGraphic">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="How much money you want to deposit?">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtAmount" layoutX="73.0" layoutY="67.0" prefHeight="35.0" prefWidth="304.0" promptText="amount here">
         <font>
            <Font size="30.0" />
         </font>
      </TextField>
      <Button fx:id="btnDeposit" layoutX="246.0" layoutY="157.0" mnemonicParsing="false" onAction="#handleDeposit" prefHeight="35.0" prefWidth="130.0" text="Deposit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="73.0" layoutY="157.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="35.0" prefWidth="130.0" text="Cancel">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
